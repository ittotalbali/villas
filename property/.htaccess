Options -Indexes
RewriteEngine On

# Optional: Handle bare /property/ by rewriting to index.php (no slug)
RewriteRule ^$ index.php [L]

# Rewrite /property/foo to index.php?slug=foo (existing rule)
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php?slug=$1 [QSA,L]